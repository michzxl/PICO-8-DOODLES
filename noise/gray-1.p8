__lua__
t=.1

pl={
	2+128,
	5+128,
	5,
	6+128,
	6,
	7
}
for i=1,#pl do
	pal(i,pl[i],1)
end

function sqr(x) return x*x end

a=30
b=30
cc=1
dd=1

::♥::
t+=0.01
if t%1.5<0.2 then
	a=rnd(30)+40
	b=rnd(30)+40
	cc=rnd(1)<.5 and 1 or -1
	cc=rnd(1)<.5 and 1 or -1
end

for i=1,1000 do
	x=rnd(128)
	y=rnd(128)
	
	c=(sin((cc*x+100)%(a)/80-t + (dd*y%b/100) + t*1.5*(b/45))/2+0.5)*(#pl+1)
	
	if c~=0 then
		circ(x,y,1,c)
	end
end

if t%1.5<0.2 then
	local x,y = rnd(128),rnd(128)
	fillp(0b1010010110100101.1)
	rectfill(x,y,128-x,128-y,0)
	fillp()
end

flip() goto ♥
