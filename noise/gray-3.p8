pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
t=.1

pl={
	2+128,
	5+128,
	5,
	6+128,
	6,
	7
}
for i=1,#pl do
	pal(i,pl[i],1)
end

function sqr(x) return x*x end

a=30
b=30
cc=1
dd=1

cls()
::♥::
t+=0.01


if t%1.5<0.2 then
	a=rnd(30)+40
	b=rnd(30)+40
	cc=rnd(1)<.5 and 1 or -1
	dd=rnd(1)<.5 and 1 or -1
	
	ti=t%1.5
	y=ti/0.2*128
	line(0,y,128,y,7)
	line(0+y,y+1,128-y,y+1,7)
	line(0+y,128-y,128-y,0-y,7)
end

for i=1,1000 do
	ox=rnd(128)
	oy=rnd(128)
	
	x=ox-4*sin(t/8-t%4*2-64+abs(oy-64)/32)
	y=oy%(ox/2+sin(t/8)*32)
	
	c=0.5+1/2*
		sin(
			  (cc*x+100)%a/80
			+ (dd*y%b/100) 
			+ t*1.5*(b/45)
			- t
		)
	
	
	c=c*(#pl+1)
	
	if c~=0 then
		circ(ox,oy,1,c)
	end
end

if t%1.5<0.2 then
	local x,y = rnd(128),rnd(128)
	fillp(0b1010010110100101.1)
	rectfill(x,y,128-x,128-y,0)
	fillp()
end

flip() goto ♥
