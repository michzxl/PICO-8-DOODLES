pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
⧗=0

cs={1,5,6,7,13}
for i=1,#cs do
	pal(i,cs[i],1)
end

cls()
::⌂::
⧗+=0.0167

for i=1,1000 do
	x,y=flr(rnd(128)),flr(rnd(128))
	if x==0 or y==0 then
		if rnd(1)<.1 then
			pset(x,y,⧗%5+1)
		end
	elseif pget(x+1,y-1)==3  then
		if rnd(1)<0.8 then
			circ(x,y,1,3)
			--pset(x-1,y+1,1,3)
		else
			circ(x,y,1,⧗%20%5+1)
		end
	elseif pget(x-1,y-1)~=0 and pget(x-1,y-1)~=3 then
		cc=pget(x-1,y-1)
		circ(x,y,1,cc)
		--pset(x+1,y+1,cc)
	end
end

