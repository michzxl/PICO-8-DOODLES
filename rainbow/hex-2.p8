hexx=hex_maker(55,60,18)

rectfill(0,0,128,128,5)
::♥::
⧗+=dt
t+=dt

for i=1,500 do
	x,y=rnd(128),rnd(128)
	xa=x-64
	ya=y-64

	c=sin(y/147-t/4)*x/64
	 *cos(x/64 -t/2)*y/128
	 +(-x/32+y/32)--bars
	 
	c=flr(c%2)==0 and 5 or 13

	circ(x,y,1,c)
end

for x=0,6 do
	for y=0,6 do
		h=(sin(x/36+(y+⧗)/12+⧗/2)+2)*12
		c=(x/1.02+y/8+⧗/5)*8
		c=c%8+8
		
		hexx(x,y,h,c)
	end
end

