version 29
__lua__
palettes={
	[0]={
		0,
		1+128,
		1,
		12+128,
		12,
		7,
	},
	{
		1+128,
		1,
		3+128,
		3,
		11+128,
		11,
		10+128,
		10,
		7+128,
		7,
	},{
