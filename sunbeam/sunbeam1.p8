pico-8 cartridge // http://www.pico-8.com
version 19
__lua__
░=750
⧗=0
speed=.5

p={7+128,10,9,9+128,8,8+128,
			2,2+128,7,12,12+128,1,1+128}
for i=1,#p do pal(i,p[i],1) end

-- helper func
function t(a)
	return flr(⧗ * a*speed)
end

function sqr(a)
	return a*a
end

rectfill(0,0,128,128,1)
::⌂::
⧗+=1/60

for i=1,░ do
	x,y=rnd(128),rnd(128)
	
	c=t(1)*flr(x/8)
	 +t(2)*flr(y/8)
	
	dist=sqrt(sqr(x-64)+sqr(y-64))
	if dist<40 then
		c=c*-1
		c=c%5+9
	elseif dist<50 then
		c=8
	else
		c=c%8+1
	end
	
	circ(x,y,1,c)
end

-- draw the little face
x=10*cos(⧗-.25)+64
y=10*sin(⧗/2)  +64
circfill(x,y,4,1)
pset(x-2,y-1,12)
pset(x+2,y-1,12)
line(x-2,y+1,x+2,y+1,12)

-- cpu usage
-- print(stat(1),0,0,10)

flip()goto ⌂
