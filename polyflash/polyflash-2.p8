	pal(1,rnd(1)<.5 and rnd(15)+1 or 128+rnd(15)+1)
end

--aba=flr(█+2800*abs(tv))
aba=1050
for i=1,aba do
	ang,r=rnd(1),rnd(rmax+pen)
	x,y=cos(ang)*r,sin(ang)*r
	
	sr=shape(ang)
	if r<sr then
		a=16*(sin(⧗/20)+1.5)*2
