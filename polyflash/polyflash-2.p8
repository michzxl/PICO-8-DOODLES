--triangle wave, w/ period 1
function tri_wave(x)
	return abs((x+.5)%1-.5)
end

--n-gon
function shape(ang)
	a=rmax*cos(0.5/n)
	b=cos(tri_wave(n*ang-0.5-t/2)/n)
	return a/b
end

function stats()
	print(flr(100*stat(1)).."%★"
  .."\n"..aba
		,0,0,7)
end

function close(a,b,thresh)
 return abs(a-b)<thresh
end

