for i=1,1000 do
	ang,r=rnd(1),rnd(64)
	x,y=64+r*cos(ang),64+r*sin(ang)
	a,b,p=20,25,sin(t/16)*0.1+0.3
	r_=2*a*abs((ang+t/8)/p%1-1/2)-a/2+b
	c=0
	if r<r_ then 
		c=r/8-ang*4+t
		c=flr(c)
		c=c+ang*6
		c=c%8+8
	elseif r<r_+5 then c=t%5+1
	elseif r<r_+8 then c=1
	else 
		c=t/2+r/16
		c=c%3+5
	end

	circ(x,y,1,c)
end

hh=flr(sin(t/16)*7+10)
for i=1,5 do
	aa,r=flr(rnd(hh)),64
	ang=aa/hh+t/8
	ang2=(aa+1)%hh/hh+t/8
	r=r-rnd(5)
	x1,y1=64+r*cos(ang),64+r*sin(ang)
	x2,y2=64+r*cos(ang2),64+r*sin(ang2)
	c=t/2+r/16
	c=(c+1)%3+5
	line(x1,y1,x2,y2,c)
end


