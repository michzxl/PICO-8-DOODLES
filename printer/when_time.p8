function isfinished(tx)
	return ti.idx>=#s
end

function new_char_pos(mx,my,lastcol)
	mx+=1
	if lastcol then
		mx,my=0,my+1
	end
	return mx,my
end

function get_chars(s,idx)
	return char(s,idx),fsget(char(s,idx)),char(s,idx-1),char(s,idx-2),char(s,idx+1)
end

function skip_trailing_spaces(s,idx)
	ix=idx
	while char(s,ix)==' ' do
		ix+=1
	end
	return ix,ix~=idx
end

function isalpha(char)
	return fsget(char)<=51
end

function char(s,i)
	return sub(s,i,i)
end

function fsget(char)
	return fontidx[char]
end

function new_sentence(cur,pre)
	curs=fsget(cur)
	return curs<=26
	 and (pre=='.' or pre=='?')
end

