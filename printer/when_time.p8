--writes some text to the screen. coroutine can be accessed by .txtr
--s : string to write
--x,y : initial x,y position.
--cw,ch : max width,height in terms of # of chars
--wait : #frames to wait between drawing chars. can be float.
--cfg : table of possible settings. currently supported:
--	cfg.usecaps : Capitalize start of sentences.
--	cfg.allcaps : ONLY CAPITAL LETTERS.
--[[SPECIAL CHARACTERS:
	… : ellipsis
--]]
function t_txtr(s,xy,wh,wait,cfg)
	local x,y,w,h=0,0,0,0
	if xy.x and xy.y then
		x,y=xy.x,xy.y
	else
		x,y=xy[1],xy[2]
	end
	if wh.w and wh.h then
		w,h=wh.w,wh.h
	else
		w,h=wh[1],wh[2]
	end
	return make_txtr(s,x,y,w,h,wait,cfg)
end

function make_txtr(s,x,y,cw,ch,wait,cfg)
	local tx={
		s=s,x=x,y=y,w=cw,h=ch,wait=wait,cfg=cfg or {},
		idx=1,ti=0,mx=0,my=0,
		ix=1,mx=0,my=0,
		finished=false,
		go=nil
	}
	tx.go = cocreate(function()
		while true do
			tx.ix,tx.mx,tx.my=1,0,0
			while tx.ix<=tx.idx do
				draw_single_char(tx)
			end
			printh("exit while")
			upd_timer(tx)
			--tx.finished = isfinished(tx)
			yield()
		end
	end)
	return tx
end

function draw_single_char(tx)
	printh("draw_single_char")
	s,x,y,w,h,cfg,idx,mx,my=tx.s,tx.x,tx.y,tx.w,tx.h,tx.cfg,tx.ix,tx.mx,tx.my
	curr,fs,prev,prev2,next=get_chars(s,idx)
	local move_cursor=false

	-- first column
	if mx==0 then
		idx,skipped=skip_trailing_spaces(s,idx)
		if (skipped) curr,fs,prev,prev2,next=get_chars(s,idx)
	end

	-- capitals
	if (cfg.allcaps and isalpha(curr))
		or (cfg.use_caps and isalpha(curr) and (idx==1 or new_sentence(curr,prev2))) then
		fs+=26
	end

	printh("gg")

	lastcol=mx==w-2
	if lastcol and (prev==' ' or curr==' ') then
		spr(fsget(" "),x+mx*8,y+mx*8)
	elseif lastcol and isalpha(prev) and isalpha(curr) and isalpha(next) then
		spr(fsget('-'),x+mx*8,y+my*8)
	else
		move_cursor=true
		spr(fs,x+mx*8,y+my*8)
	end

	printh("hh")

	tx.ix=move_cursor and idx+1 or idx
	tx.mx,tx.my=new_char_pos(mx,my,lastcol)	
end

function upd_timer(tx)
	if tx.ti<1 then
		tx.idx=min(tx.ix,#s)
		tx.ti+=tx.wait
	end
	tx.ti=tx.ti-1
end

