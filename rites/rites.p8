version 18
__lua__
█=6000
⧗=0
num=5
dist=50
shape=2

function sqr(a) return a*a end

pal(9,14+128,1)
pal(8,15+128,1)
pal(3,1+128,1)
pal(12,8,1)

cls()
::⌂::
pp={}
⧗+=0.0166

rr=rnd(1)<0.1
for i=1,█ do
	x,y=rnd(108)+10,rnd(128)
	pset(x,y,0)
end

for i=1,num do
	ang=i/num
	local x=dist*1*cos(ang+⧗/4)+64
	local y=dist*0.65*sin(ang+⧗/4)+64
	add(pp,{x=x,y=y})
end

for i=1,num do
	x,y=pp[i].x,pp[i].y
	
	ss=(i+num/shape)%num+1
	p1=pp[flr(ss)]
	p2=pp[flr(ss-1)%num+1]
	
	line(x,y+40+rnd(3),p2.x,p2.y+40+rnd(3),3)
	line(x,y+20+rnd(3),p2.x,p2.y+20+rnd(3),1)
	line(x,y,p1.x,p1.y,7)
	line(x,y,p2.x,p2.y,7)
end

for i=1,num do
	x,y=pp[i].x,pp[i].y
	rectfill(x-2,y-5,x+2,y,9)
	circfill(x,y,2,9)
	circfill(x,y-5,2,8)
end

circ(63,10*sin(⧗/2+.02)+30,5,12)
circ(63,10*sin(⧗/2)+30,5,12)


flip()goto ⌂
