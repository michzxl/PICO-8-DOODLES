	line(x,y,p1.x,(p1.y+p2.x)/2,10)
	line(x,y,p2.x,(p2.y+p1.x)/2,10)
end

for i=1,5 do
	x,y=pp[i].x, pp[i].y+8*cos(t/8+i/8)-4*sin(t/2)
	x1,y1 = x-4,y-4
	for i=1,3 do
