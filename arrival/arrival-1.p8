	pal(2,0,1)

	cls()
end

function _update60()
	t+=dt

	for i=1,750 do
		local x = rnd(128)
		local y = rnd(128)

		local fill = fills[max(1,flr((
			x/320
			+ 0.3 * sin(t/8) 
			+ 0.05 * sin(x / (64 + 8*sin(t/8))) 
			+ (y-16) / 160
			- flr(x/16 + y/32)*sin(t/16)/16
			- flr(-x/32+y/32+t/8)%2/4
		)*(#fills)))]

		local c =
			x/64 * sin(y/147-t/4)
			+ (-x/32 + y/32)
		c %= 2

		fillp(fill)
		circ(x,y,1,c)
		fillp()
	end
end
